{
    "name":     "psr7-sessions/storageless",
    "description": "Storageless PSR-7 Session support",
    "license":  "MIT",
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.io/",
            "role":     "Developer"
        },
        {
            "name":  "Jefersson Nathan",
            "email": "malukenho@phpse.net",
            "role":  "Developer"
        },
        {
            "name":  "Luís Otávio Cobucci Oblonczyk",
            "email": "lcobucci@gmail.com",
            "role":  "Developer"
        }
    ],
    "require": {
        "php":                             "~8.0.0 || ~8.1.0",
        "ext-json":                        "*",
        "dflydev/fig-cookies":             "^3.0.0",
        "lcobucci/jwt":                    "^4.1.5",
        "lcobucci/clock":                  "^2.1.0",
        "psr/http-message":                "^1.0.1",
        "psr/http-server-handler":         "^1.0.1",
        "psr/http-server-middleware":      "^1.0.1"
    },
    "require-dev": {
        "doctrine/coding-standard":               "^9.0.0",
        "laminas/laminas-diactoros":              "^2.9.2",
        "laminas/laminas-httphandlerrunner":      "^2.1.0",
        "phpunit/phpunit":                        "^9.5.20",
        "psalm/plugin-phpunit":                   "^0.16.1",
        "roave/infection-static-analysis-plugin": "^1.18.0",
        "squizlabs/php_codesniffer":              "^3.6.2",
        "vimeo/psalm":                            "^4.22.0"
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "ocramius/psr7-session": "self.version"
    },
    "autoload": {
        "psr-4": {
            "PSR7Sessions\\Storageless\\": "src/Storageless"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PSR7SessionsTest\\Storageless\\": "test/StoragelessTest"
        }
    }
}
